DECLARE
I NUMBER(3);
J NUMBER(3);
BEGIN
I:=2;
LOOP
J:=2;
LOOP
EXIT WHEN MOD(I,J)=0 OR (J=I);
J:=J+1;
END LOOP;
IF(J=I) THEN
DBMS_OUTPUT.PUT_LINE(I||' IS PRIME');
END IF;
I:=I+1;
EXIT WHEN I=50;
END LOOP;
END;
/







