DECLARE
S NUMBER:=&S;
E NUMBER:=&E;
BEGIN
DBMS_OUTPUT.PUT_LINE('THE NUMBERS B/W'||S||' AND '||E||' ARE ');
WHILE(S<=E)
LOOP
DBMS_OUTPUT.PUT_LINE(S);
S:=S+1;
END LOOP;
END;
/












EXP-12

DECLARE
N NUMBER;
S NUMBER:=0;
R NUMBER;
K NUMBER;
BEGIN
N:=&N;
K:=N;
LOOP
EXIT WHEN N=0;
S:=S*10;
R:=MOD(N,10);
S:=S+R;
N:=TRUNC(N/10);
END LOOP;
DBMS_OUTPUT.PUT_LINE('THE INVERTED NUMBER OF '||K||' IS '||S);
END;
/

