DECLARE
A VARCHAR2(5);
B NUMBER;
C VARCHAR2(5);
I NUMBER;
BEGIN
A:='&NUM';
B:=LENGTH(A);
FOR I IN REVERSE 1..B
LOOP
C:=C||SUBSTR(A,I,1);
END LOOP;
DBMS_OUTPUT.PUT_LINE('INVERTED NUMBERS IS '||C);
END;
/






